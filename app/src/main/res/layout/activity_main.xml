<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

        <Button
            android:id="@+id/leadTwelveBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Lead-12"
            android:layout_marginTop="@dimen/_5sdp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>


        <TextView
            android:id="@+id/testResultTv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:hint="Result Test"
            android:scrollbars="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/leadTwelveBtn" />

        <TextView
            android:id="@+id/pdf_link_url"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:autoLink="web"
            app:layout_constraintTop_toBottomOf="@id/testResultTv"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>